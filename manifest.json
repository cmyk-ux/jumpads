{
    "manifest_version": 2,

    "name"       : "JumpADS",
    "version"    : "0.6.14.1",
    "description": "Pule anúncios que seu canal favorito do YouTube sempre coloca nos vídeos.",

    "icons": {
	"16" : "assets/jads16.png",
        "48" : "assets/jads48.png",
        "128": "assets/jads128.png"
    },

    "browser_action": {
	"default_popup": "index.html"
    },

    "background": {
	"scripts": ["src/main.js"]
    },
    
    "content_scripts": [
	{
	    "matches": ["*://*.youtube.com/*"],
	    "js"     : ["src/youtube.js"]
	}
    ],

    "web_accessible_resources": ["src/backends/*.js"],

    "build_settings": {
	"enabled_backends": ["example"]
    }
}
